{
  "capabilities":
  [
    {
      "browserName": "firefox",
      "marionette": true,
      "maxInstances": 3,
      "platform": "WINDOWS",
      "seleniumProtocol": "WebDriver",
      "webdriver.firefox.driver": "D:/webdriver/Geckodriver/geckodriver.exe"
    },
    {
      "browserName": "internet explorer",
      "platform": "WINDOWS",
      "maxInstances": 1,
      "seleniumProtocol": "WebDriver",
      "webdriver.ie.driver": "D:/webdriver/IEDriver/IEDriverServer.exe"
    }
  ],
  "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
  "maxSession": 5,
  "port": 5556,
  "register": true,
